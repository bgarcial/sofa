# CMake package configuration file for the SofaPython27 plugin

@PACKAGE_INIT@

find_package(PythonLibs 2.7 REQUIRED)
sofa_create_target(PythonLibs SofaPython27 "${PYTHON_LIBRARIES}" "${PYTHON_INCLUDE_DIRS}")
find_package(SofaGui REQUIRED)
find_package(SofaGeneral REQUIRED)
find_package(SofaMisc REQUIRED)

set(SOFA_HAVE_PYTHON "@SOFA_HAVE_PYTHON@")

if(NOT TARGET SofaPython27)
	include("${CMAKE_CURRENT_LIST_DIR}/SofaPythonTargets.cmake")
endif()

check_required_components(SofaPython27)
